{
  "name": "shzam",
  "version": "0.1.0",
  "description": "Nodejs-expressjs starter kit with boilerplate code",
  "main": "./bin/index.ts",
  "bin": {
    "shzam": "./bin/index.ts"
  },
  "scripts": {
    "build": "rimraf ./build && tsc --incremental && tsc-alias",
    "start": "ts-node ./bin/index.ts",
    "eslint": "eslint . --ext .js,.ts",
    "upgrade": "npm update --save-dev && npm update --save",
    "test": "jest --detectOpenHandles --forceExit --coverage --verbose",
    "lint:fix": "eslint src --fix --ext .js,.jsx,.ts,.tsx .",
    "nibble": "eslint-nibble src --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install",
    "prepare-ts": "npx ts-patch install -s"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.2.5",
    "@types/listr": "^0.14.4",
    "@types/nconf": "^0.10.3",
    "@types/ncp": "^2.0.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint-nibble": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "module-alias": "^2.2.2",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.1.2",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "dotenv": "^16.0.3",
    "i": "^0.3.7",
    "inquirer": "^8.0.0",
    "joi": "^17.7.0",
    "listr": "^0.14.3",
    "nconf": "^0.12.0",
    "ncp": "^2.0.0",
    "npm": "^9.8.1",
    "pkg-install": "^1.0.0",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shzam/CLI.git"
  },
  "bugs": {
    "url": "https://github.com/shzam/CLI/issues"
  },
  "homepage": "https://github.com/shzam/node-express-starter-kit"
}
